
public abstract class Line extends Shape {
	private Point start;

	public Line(char paintCharacter, int x, int y) {
		super(paintCharacter);
		start = new Point(x,y);
	}
		
		public Line(char paintCharacter, Point point) {
			super(paintCharacter);
			start = point;
		
		
		// TODO Auto-generated constructor stub
	}

	public abstract void paintOn(Screen screen);



}
