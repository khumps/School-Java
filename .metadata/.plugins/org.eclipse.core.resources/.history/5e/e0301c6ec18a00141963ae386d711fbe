
public abstract class Line extends Shape {
	protected Point start;
	protected int length;

	public Line(char paintChar) {
		super(paintChar);
		start = new Point(x,y);
		this.length = length;
		
	}
		
		public Line(char paintChar) {
			super(paintChar);
			
			this.length = length;
			
		
		
		// TODO Auto-generated constructor stub
	}

	public abstract void paintOn(Screen screen);



}
