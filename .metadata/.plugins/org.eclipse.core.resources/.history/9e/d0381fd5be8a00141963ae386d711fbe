
public abstract class Line extends Shape {
	private Point start;

	public Line(int x, int y, char paintChar) {
		super(paintChar);
		start = new Point(x,y);
	}
		
		public Line(Point point, char paintChar) {
			super(paintChar);
			start = point;
		
		
		// TODO Auto-generated constructor stub
	}

	public abstract void paintOn(Screen screen);



}
