
public class StatisticsTest {

	public static void main(String[] args) {
		int i = 0;
		for(double[] data = new double[10 + i]; i < 30; i += 10)
		{
				for (int j = 0; j < data.length; j++) {
					data[j] = (double)Math.random() * 100;
				}
				System.out.println(Statistics.arrayToString(data));
		}
		
	}

}
