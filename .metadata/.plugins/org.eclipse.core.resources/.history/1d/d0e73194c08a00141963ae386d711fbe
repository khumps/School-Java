
public abstract class Line extends Shape {
	final protected Point start;
	final protected int length;

	public Line(int x, int y, int length, char paintChar) {
		super(paintChar);
		this.length = length;
		start = new Point(x,y);
		
	}
		
		public Line(Point point,int length, char paintChar) {
			super(paintChar);
			this.length = length;
			start = point;
			
		
		
		// TODO Auto-generated constructor stub
	}

	public abstract void paintOn(Screen screen);



}
